                      </div>
                    </div>
                  </div>
                </div>
              </div>

              <!-- Instructor Section -->
              <div class="tab-pane fade" id="instructor" role="tabpanel" aria-labelledby="instructor-tab">
                <div class="course-tab-content">
                  <div class="course-author-wrapper">
                    <div class="thumbnail">
                      <%# <img src="assets/images/instructor/course-details/instructor-2.jpg" alt="Author Images"> %>
                      <% if @course.user %>
                        <% if @course.user.image_url %>
                          <%= image_tag @course.user.image_url, size: '30x30', class: 'rounded-circle mr-2' %>
                        <% end %>
                      <% end %>
