<% content_for :title, "Dashboard" %>

<!DOCTYPE html>
<html>
  <head>
  	<meta charset="utf-8">
    <title>Brainstaq | <%= yield (:title) %></title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    
    <meta name="viewport" content="width=device-width, initial-scale=1">
  	<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <%= tinymce_assets %>
    <script type="text/javascript" src="/assets/tinymce.js"></script>
    <%= favicon_link_tag asset_path('favicon.ico') %>
    <%= stylesheet_link_tag 'dashboard', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_pack_tag 'dashboard', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag '//ajax.googleapis.com/ajax/libs/jqueryui/1.12.1/themes/le-frog/jquery-ui.min.css' %>
    <%= javascript_pack_tag 'dashboard', 'data-turbolinks-track': 'reload' %>
    <%#= yield :recaptcha_js %>
  </head>

  <body>
		<%# <%= render partial: "layouts/user_header", locals: { users: @users} %>
		<%= render partial: "layouts/flash" %>
		<%= yield %>
		<%= intro_tags %>
		<%= render partial: "layouts/user_footer", locals: { users: @users} %>	
		
  </body>
</html>