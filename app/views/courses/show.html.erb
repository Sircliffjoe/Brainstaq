          <div id="collapse-<%= index + 1 %>" class="accordion-collapse collapse" aria-labelledby="heading-<%= index + 1 %>" data-parent="#accordionExample2">
            <div class="accordion-body mt-3">
              <% chapter.lessons.rank(:row_order).each do |lesson| %>
