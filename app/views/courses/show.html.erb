<div class="tab-pane fade" id="curriculum" role="tabpanel" aria-labelledby="curriculum-tab">
  <!-- Course accordion START -->
  <div class="accordion accordion-icon accordion-bg-light" id="accordionExample2">
    <% if current_user %>
      <% if @course.user == current_user %>
        <% @chapters.each_with_index do |chapter, index| %>
          <div class="accordion-item mb-3">
            <h6 class="accordion-header font-base" id="heading-<%= index + 1 %>">
              <button class="accordion-button fw-bold rounded d-sm-flex d-inline-block collapsed" type="button" data-toggle="collapse" data-target="#collapse-<%= index + 1 %>" aria-expanded="false" aria-controls="collapse-<%= index + 1 %>">
                <i class="far fa-bookmark"></i> Chapter <%= chapter.row_order_rank + 1 %>: <%= chapter.title %>
              </button>
            </h6>
            <div id="collapse-<%= index + 1 %>" class="accordion-collapse collapse" aria-labelledby="heading-<%= index + 1 %>" data-parent="#accordionExample2">
              <div class="accordion-body mt-3">
                <% chapter.lessons.rank(:row_order).each do |lesson| %>
                  <div class="d-flex justify-content-between align-items-center">
                    <div class="position-relative d-flex align-items-center">
                      <a href="#" class="btn btn-danger-soft btn-round btn-sm mb-0 stretched-link position-static">
                        <i class="fas fa-play me-0"></i>
                      </a>
                      <span class="d-inline-block text-truncate ms-2 mb-0 h6 fw-light w-100px w-sm-200px w-md-400px">
                        <%= link_to course_lesson_path(@course, lesson, anchor: "current_lesson") do %>
                          <%= lesson.row_order_rank + 1 %>. <%= lesson.title %>
                        <% end %>
                      </span>
                    </div>
                    <% if current_user.admin? %>
                      <%= link_to edit_course_lesson_path(@course, lesson), class: 'badge badge-warning mr-1' do %>
                        <i class="fa fa-edit"></i>
                      <% end %>
                      <%= link_to [@course, lesson], method: :delete, data: { confirm: 'Are you sure?' }, class: 'badge badge-danger' do %>
                        <i class="fa fa-trash"></i>
                      <% end %>
                    <% elsif current_user.viewed?(lesson) %>
                      <div class="text-success"><i class="fa fa-check-square"></i></div>
                    <% else %>
                      <div class="text-danger"><i class="fa fa-check-square"></i></div>
                    <% end %>
                    <div class="row">
                      <% unless lesson.comments_count.zero? %>
                        <i class="fa fa-comment"></i> <%= lesson.comments_count %>
                      <% end %>
                    </div>
                  </div>
                  <hr> <!-- Divider -->
                <% end %>
              </div>
            </div>
          </div>
        <% end %>
