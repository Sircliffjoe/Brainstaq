                    </a>
                    <span class="d-inline-block text-truncate ms-2 mb-0 h6 fw-light w-100px w-sm-200px w-md-400px">
                      <%= link_to course_lesson_path(@course, lesson, anchor: "current_lesson") do %>
                        <%= lesson.row_order_rank + 1 %>. <%= lesson.title %>
                      <% end %>
                    </span>
                  </div>
                </div>
                <hr> <!-- Divider -->
              <% end %>
            </div>
          </div>
        </div>
      <% end %>
    <% end %>
