                      <%= image_tag @course.user.image_url, size: '30x30', class: 'rounded-circle mr-2' %>
                    <% end %>
                  <% end %>
                  <span class="author-title">By <%= link_to @course.user.full_name, profile_path(@course.user.username) %></span>
                </div>
              </div>
              <div class="edu-rating rating-default">
